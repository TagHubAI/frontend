{
  "name": "turborepo-basic-pnpm",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "build:website": "turbo run build --filter website",
    "dev": "turbo run dev --parallel",
    "dev:website": "turbo run dev --filter website",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "prettier": "latest",
    "turbo": "latest"
  }
}
